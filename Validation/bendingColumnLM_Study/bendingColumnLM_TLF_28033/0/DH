FoamFile
{
    version     2.0;
    format      ascii;
    class       pointTensorField;
    location    "0";
    object      DH;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (1 0 0 0 1 0 0 0 1);

boundaryField
{
    free
    {
        type            calculated;
    }
    fixed
    {
        type            calculated;
    }
    freeTop
    {
        type            calculated;
    }
}


