FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      t;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    free    { type fixedValue; value uniform (30000 0 0); }
    freeTop { type fixedValue; value uniform (30000 0 0); }
    fixed   { type fixedValue; value uniform (    0 0 0); }
}

