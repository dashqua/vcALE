FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application vcALEFoam;

startFrom startTime; // latestTime;

startTime 0;

stopAt endTime;

endTime 2.0;

timeStepping constant; 

cfl 0.4;

RungeKuttaStages 2;

deltaT 1e-4;

writeControl runTime;

writeInterval 0.01;

purgeWrite 0;

writeFormat ascii;

writePrecision 8;

writeCompression off;

timeFormat general;

timePrecision 6;

graphFormat raw;

runTimeModifiable false;

