FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     vcALEFoam;

startFrom 	latestTime;

startTime 	0;

stopAt 		endTime;

endTime 	8e-5;

timeStepping 	constant; //variable;

cfl 		0.4;

deltaT 		1e-08; //0;

writeControl 	runTime;

writeInterval 	1e-7; 

purgeWrite 	0;

writeFormat 	ascii;

writePrecision 	8;

writeCompression off;

timeFormat 	general;

timePrecision 	6;

graphFormat 	raw;

runTimeModifiable false;

