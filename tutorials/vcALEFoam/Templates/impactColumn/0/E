FoamFile
{
    version     2.0;
    format      ascii;
    class       pointScalarField;
    location    "0";
    object      E;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 17000000;

boundaryField
{
    roller_symmetricY
    {
        type            zeroGradient;
    }
    roller_symmetricZ
    {
        type            zeroGradient;
    }
    roller_symmetricX
    {
        type            zeroGradient;
    }
    free
    {
        type            zeroGradient;
    }
}


