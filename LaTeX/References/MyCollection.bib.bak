file = {:C$\backslash$:/Users/thomas/Dropbox/Thomas{\_}Di{\_}Giusto-PhD/References/Papers/OpenFoam{\_}Courses/2016{\_}Haider{\_}Large{\_}strain{\_}solid{\_}dynamics{\_}in{\_}OpenFOAM.pdf:pdf},
journal = {User OpenFOAM conference 2016},
