journal = {OpenFOAM Workshop},
keywords = {1,a simplified formulation of,and provides source,are commonly based on,barotropic model,cavitating flows,cavitation,cfd codes,density-based method,equation is derived from,for the simulation of,godunov scheme,methods in commercially available,single vapour bubble,the dynamics of a,the rayleigh-plesset equation,this},
