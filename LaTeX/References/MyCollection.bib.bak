@article{Persson2009,
abstract = {We describe a method for computing time-dependent solutions to the compressible Navier-Stokes equations on variable geometries. We introduce a continuous mapping between a fixed reference configuration and the time-varying domain. By writing the Navier-Stokes equations as a conservation law for the independent variables in the reference configuration, the complexity introduced by variable geometry is reduced to solving a transformed conservation law in a fixed reference configuration. The spatial discretization is carried out using the Discontinuous Galerkin method on unstructured meshes of triangles, while the time integration is performed using an explicit Runge-Kutta method. For general domain changes, the standard scheme fails to preserve exactly the free-stream solution which leads to some accuracy degradation, especially for low order approximations. This situation is remedied by adding an additional equation for the time evolution of the transformation Jacobian to the original conservation law and correcting for the accumulated metric integration errors. A number of results are shown to illustrate the flexibility of the approach to handle high-order approximations on complex geometries. {\textcopyright} 2009 Elsevier B.V. All rights reserved.},
author = {Persson, P. O. and Bonet, J. and Peraire, J.},
doi = {10.1016/j.cma.2009.01.012},
file = {:C$\backslash$:/Users/thomas/Dropbox/Thomas{\_}Di{\_}Giusto-PhD/References/Papers/Mathematics/2009{\_}Bonet{\_}Discontinuous{\_}Galerkin{\_}solution{\_}of{\_}the{\_}Navier-Stokes{\_}equations{\_}on{\_}deformable{\_}domains.pdf:pdf},
isbn = {1563478900},
issn = {00457825},
journal = {Computer Methods in Applied Mechanics and Engineering},
keywords = {Arbitrary Lagrangian-Eulerian,Deformable domains,Discontinuous Galerkin,Geometric conservation,Navier-Stokes},
number = {17-20},
pages = {1585--1595},
publisher = {Elsevier B.V.},
title = {{Discontinuous Galerkin solution of the Navier-Stokes equations on deformable domains}},
url = {http://dx.doi.org/10.1016/j.cma.2009.01.012},
volume = {198},
year = {2009}
}
@article{Jasak2009b,
abstract = {Extension of static mesh numerics in a CFD solver to cases with deforming boundaries considerably expands the scope of its use. Dynamic mesh handling includes deforming mesh cases, where the number and connectivity of mesh elements remains unchanged; and topological changes, where mesh size and connectivity varies during the simulation. Cases where the boundary deformation itself represents a part of the solution demand special attention: here, mesh handling needs to be fully automatic. This paper describes dynamic mesh support in OpenFOAM, a C++ object-oriented library for numerical simulations in continuum mechanics. Unlike other tools, where dynamic mesh support is usually retro-fitted, object-oriented dynamic mesh engine has been built up from ground-up. Emphasis is given polyhedral cell support in mesh analysis and discretisation, vertex-based automatic mesh motion techniques and hierarchical design of topology morphing engine. The paper is completed with examples of solution-dependent motion with large boundary deformation. Copyright {\textcopyright} 2009 by the American Institute of Aeronautics and Astronautics, Inc.},
author = {Jasak, Hrvoje},
doi = {10.2514/6.2009-341},
file = {:C$\backslash$:/Users/thomas/Downloads/Dynamic{\_}Mesh{\_}Handling{\_}in{\_}OpenFOAM.pdf:pdf},
isbn = {9781563479694},
journal = {47th AIAA Aerospace Sciences Meeting including the New Horizons Forum and Aerospace Exposition},
