journal = {International Journal of Impact Engineering},
keywords = {Arbitrary Lagrangian Eulerian,Blast,Fluid-structure interaction,Multi-material,Wave propagation},
number = {10-11},
pages = {1269--1275},
publisher = {Elsevier Ltd},
title = {{Numerical analysis of blast-induced wave propagation using FSI and ALEmulti-material formulations}},
url = {http://dx.doi.org/10.1016/j.ijimpeng.2009.03.007},
volume = {36},
year = {2009}
}
